{
	"manifest_version": 3,
	"name": "atcoder-tl-searcher",
	"version": "1.0",
	"description": "AtCoderの問題ページに、コンテスト直後のAtCoder TLへのリンクを追加します。",
  "content_scripts": [{
    "matches":[
      "https://atcoder.jp/contests/*"
    ],
    "js": ["js/bundle.js"]
  }],
  "web_accessible_resources": [{
    "resources": ["icon/*", "bootstrap.min.js.map"],
    "matches": ["<all_urls>"]
  }], 
  "permissions": ["storage"]
}